{
  "name": "@weave-js/core",
  "version": "0.7.0-rc.6",
  "description": "The core package of weave",
  "keywords": [
    "Weave",
    "Microservice",
    "Framework",
    "Docker",
    "SAAS"
  ],
  "main": "./lib/index.js",
  "scripts": {
    "ci": "jest --watch",
    "test": "NODE_ENV=test jest --no-cache",
