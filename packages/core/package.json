    "coverage": "jest --coverage",
    "analyse": "eslint lib/**",
    "eslint": "eslint --ext=.js lib test dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fachw3rk/weave"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "weave": "./bin/weave.js"
  },
  "author": {
    "name": "Kevin Ries",
    "company": "Fachwerk Software",
    "email": "kevin.ries@fachwerk.io",
    "url": "https://weave-js.com"
  },
  "license": "MIT",
  "dependencies": {
    "args": "^5.0.1",
    "carrier": "^0.3.0",
    "cli-table": "^0.3.1",
    "es6-error": "^4.1.1",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eventemitter2": "^5.0.1",
    "fachwork": "0.0.10",
    "figures": "^3.0.0",
    "fw-object-validator": "^0.2.0",
    "glob": "^7.1.4",
    "ioredis": "^4.9.5",
    "ip": "^1.1.5",
    "kleur": "^3.0.3",
    "lodash": "^4.17.11",
    "nanomatch": "^1.2.13",
    "nats": "^1.2.10",
    "node-object-hash": "^1.4.2",
    "node-strict": "^1.0.1",
    "norma": "^0.4.1",
    "uuid": "^3.3.2",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-fw": "^1.0.3",
    "eslint-loader": "^2.1.2",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "lolex": "^4.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "rootDir": "./lib",
    "roots": [
      "../test"
    ]
  },
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "gitHead": "cdd9762d78d076dc7bdfa0784cdc9ce6af6a57c3"
