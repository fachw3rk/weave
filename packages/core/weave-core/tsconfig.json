{
  "extends": "../../../tsconfig",
  "include": [
    "src/**/*.ts"
  ],
  "compilerOptions": {
    "outDir": "lib",
    "target": "es2015",
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "declaration": true,                   /* Generates corresponding '.d.ts' file. */
    "sourceMap": true
  }
}